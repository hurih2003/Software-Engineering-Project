<?xml version="1.0" encoding="UTF-8"?>
<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/19" fx:controller="ScanCamra.ScanCameraController">
    <stylesheets>
        <URL value="@scancamera.css" />
    </stylesheets>
   <children>
      <ImageView fx:id="imageView" fitHeight="243.0" fitWidth="337.0" preserveRatio="true"
                 AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="20.0" />
      <Label fx:id="label" prefHeight="18.0" prefWidth="337.0" 
             AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="270.0" />
      <Button fx:id="startCameraButton" text="Start Camera" prefWidth="140.0" prefHeight="40.0"
              AnchorPane.rightAnchor="30.0" AnchorPane.topAnchor="60.0" />
      <Button fx:id="stopCameraButton" text="Stop Camera" prefWidth="140.0" prefHeight="40.0"
              AnchorPane.rightAnchor="30.0" AnchorPane.topAnchor="120.0" />
   </children>
</AnchorPane>

